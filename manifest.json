{
    "manifest_version": 3,
    "name": "Random Image New Tab",
    "description": "This extension replaces the new tab with a random image and a search bar.",
    "version": "1.0",
    "permissions": ["storage"],
    "action": {},
    "chrome_url_overrides": {
      "newtab": "journey.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "48": "icon.png"
    }
}
